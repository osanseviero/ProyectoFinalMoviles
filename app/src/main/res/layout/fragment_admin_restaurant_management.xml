<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.osanseviero.proyectofinalmoviles.adminFragments.AdminRestaurantManagementFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/createTable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Crear Mesa" />

        <Button
            android:id="@+id/createDish"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Crear Platillo" />

        <Button
            android:id="@+id/adminCreateMaterial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Crear materia prima" />
    </LinearLayout>

</FrameLayout>
