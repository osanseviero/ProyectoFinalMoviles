<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.osanseviero.proyectofinalmoviles.ChefHomeScreenActivity">

    <Button
        android:id="@+id/chefConfig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Config"
        android:onClick="chefConfig"/>

    <Button
        android:id="@+id/chefHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:onClick="chefHome"/>

    <FrameLayout
        android:id="@+id/contentChefFragment"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1" />
</LinearLayout>
