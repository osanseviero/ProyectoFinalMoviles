<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.osanseviero.proyectofinalmoviles">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity
            android:name=".ClientHomeScreenActivity"
            android:label="@string/title_activity_client_home_scren" />
        <activity
            android:name=".WaiterHomeScreenActivity"
            android:label="@string/title_activity_waiter_home_screen" />
        <activity
            android:name=".AdminHomeScreenActivity"
            android:label="@string/title_activity_admin_home_screen" />
        <activity android:name=".ChefHomeScreenActivity" />
    </application>

</manifest>